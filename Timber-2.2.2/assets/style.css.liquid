  /* ----------------------------------------------------------------
  Media Queries
  -----------------------------------------------------------------*/

  /*1119*/
  @media screen and (min-width: 70em) {
    {%if settings.use_wide_layout%}
    .footer-image-gallery-image{height:140px;}
    .footer-image-gallery-overlay{height:140px;}
	.flexslider .med-collection-arrow{display:none;}
    {%endif%}
    
  }  
  /*1052*/
  @media screen and (max-width: 65.75em) {
    #main-nav li {margin-right:2%;}
    #footer-about-contact h4 {font-size:16px;font-size:1.6rem;{% if settings.font_choice == 'Arimo-Patua-One' %}font-size:15px;font-size:1.5rem;{% endif %}}
    #footer-about-contact h2, #footer-bottom #footer-about-contact h2 a, #footer-bottom #footer-about-contact h2 a:link, #footer-bottom #footer-about-contact h2 a:hover, #footer-bottom #footer-about-contact h2 a:visited {font-size:33px;font-size:3.3rem;{% if settings.font_choice == 'Arimo-Patua-One' %}font-size:31px;font-size:3.1rem;{% endif %}}
  	.col img {margin:0 auto 20px;}
  }
  /*972px*/
  @media screen and (max-width: 60.75em) {
    #main-header {margin-bottom:0%;}
    #main-nav li {margin-right:1.6%;}
    .footer-image-gallery-overlay{padding-top:38%;}
    {% if settings.font_choice == 'Arimo-Patua-One' %}#utility-nav a {font-size:14px;font-size:1.4rem;}{% endif %}
    {% if settings.font_choice == 'Garamond-Futura' or settings.font_choice == 'Open-Sans' or settings.font_choice == 'Helvetica-Neue' or settings.font_choice == 'Times-New-Roman'%}#main-nav li a, #main-nav li a:link, #main-nav li a:visited, .join, .join:link, .join:visited{font-size: 13px; font-size: 1.3rem; } #utility-nav li{font-size: 14px;font-size:1.4rem;}{%endif%}
    .grid-view .product,
    .grid-view .product-break {width:30%;}
    .grid-view.two-across-wrapper .product-image-wrapper {width:100%;}
    .grid-view.two-across-wrapper .product img{padding-left:7px;}
    .grid-view.two-across-wrapper .product-image-wrapper{text-align:center;}
    /*product*/
    {% if settings.use_tab_four or settings.use_tab_five %}
    #product-tabs-toolbar {border:1px #e8e8e8 solid;}
    .tab-item {display:block;font-size:16px;font-size:1.6rem;border: none !important;border-bottom: 1px solid #e8e8e8 !important;}
    .tab-item:last-child {border-bottom:none !important;}
    #product-tabs-toolbar span.tab-item-active {
      background-color: #de8686;
      color: white !important;
    }
    {% endif %}
  }
  /*932px*/
  @media screen and (max-width: 58.25em) {
    /* Header */
    #main-header {min-height: 100px;}
    .header-flags li{max-width: 20px; max-height: 15px; margin-top: 7px;}
    .header-flags li img{vertical-align: top;}
    #utility-nav{width: 38%;}
    #main-nav li {margin-right:1%;}
    {% if settings.font_choice == 'lato-oswald'%}#main-nav li {margin-right:1.4%;}{%endif%}
    {% if settings.font_choice == 'Open-Sans' or settings.font_choice == 'Helvetica-Neue' or settings.font_choice == 'Times-New-Roman'%}#main-nav li a, #main-nav li a:link, #main-nav li a:visited{font-size: 12px; font-size: 1.2rem;}{%endif%}
    .join,
    .join:link,
    .join:visited {font-size:13px;font-size:1.3rem;color:{{ settings.main_nav_link_color }};}
    {% if settings.font_choice == 'Helvetica-Neue' or settings.font_choice == 'Times-New-Roman' or settings.font_choice == 'Garamond-Futura' or settings.font_choice == 'Open-Sans'  %}    .join, .join:link, .join:visited{font-size: 12px; font-size: 1.2rem;}{%endif%}
    #header-search h4 {font-size:16px;font-size:1.6rem;}
    #header-cta .tag-line {font-size:16px;font-size:1.6rem;}
    #call-us {width:25%;}
    #call-us h3 {font-size:14px;font-size:1.4rem;}
    #call-us h3 i {font-size:10px;font-size:1.0rem;}
    #call-us h4, #call-us h4 a, #call-us h4 a:link, #call-us h4 a:hover {font-size:26px;font-size:2.6rem;}
    #footer-cta {padding:3% 0;}
    #footer-cta h2 {font-size:2.8rem;font-size:28px;} 
    .home-linklists-4 ul.home-linklist-ul{width: 49.5%;display:inline-block;vertical-align:top;}
    .home-linklists-4 ul.home-linklist-ul:nth-child(3){border-left:none;}
    #product-secondary-layout #product-images-wrapper {width:50%;}
    #product-secondary-layout #product-info {width:47%;margin-right:0;float:right;}
    #product-secondary-layout #product-options {float:left;width:100%;}
    #product-video-wrapper {width:100%;margin-top:3.5%;}
    #product-video-wrapper iframe:first-child {margin-bottom:2.5%;}
    #footer-about-contact h4 {font-size:15px;;font-size:1.5rem;{% if settings.font_choice == 'Arimo-Patua-One' %}font-size:14px;font-size:1.4rem;{% endif %}}
    #footer-about-contact h2, #footer-bottom #footer-about-contact h2 a, #footer-bottom #footer-about-contact h2 a:link, #footer-bottom #footer-about-contact h2 a:hover, #footer-bottom #footer-about-contact h2 a:visited {font-size:30px;font-size:3.0rem;{% if settings.font_choice == 'Arimo-Patua-One' %}font-size:28px;font-size:2.8rem;{% endif %}}
  }
 @media screen and (min-width: 53em) {
   #call-us h4, #call-us h4 a, #call-us h4 a:link, #call-us h4 a:hover {pointer-events: none; cursor: default;}
 }
